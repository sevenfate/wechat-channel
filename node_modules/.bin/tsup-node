#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/e/web/apk/RuoYi-Vue-Wechat/moltbot/node_modules/.pnpm/tsup@8.5.1_jiti@2.6.1_postc_73b0d079a764cbb5c51135d704e2206c/node_modules/tsup/dist/node_modules:/proc/cygdrive/e/web/apk/RuoYi-Vue-Wechat/moltbot/node_modules/.pnpm/tsup@8.5.1_jiti@2.6.1_postc_73b0d079a764cbb5c51135d704e2206c/node_modules/tsup/node_modules:/proc/cygdrive/e/web/apk/RuoYi-Vue-Wechat/moltbot/node_modules/.pnpm/tsup@8.5.1_jiti@2.6.1_postc_73b0d079a764cbb5c51135d704e2206c/node_modules:/proc/cygdrive/e/web/apk/RuoYi-Vue-Wechat/moltbot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/e/web/apk/RuoYi-Vue-Wechat/moltbot/node_modules/.pnpm/tsup@8.5.1_jiti@2.6.1_postc_73b0d079a764cbb5c51135d704e2206c/node_modules/tsup/dist/node_modules:/proc/cygdrive/e/web/apk/RuoYi-Vue-Wechat/moltbot/node_modules/.pnpm/tsup@8.5.1_jiti@2.6.1_postc_73b0d079a764cbb5c51135d704e2206c/node_modules/tsup/node_modules:/proc/cygdrive/e/web/apk/RuoYi-Vue-Wechat/moltbot/node_modules/.pnpm/tsup@8.5.1_jiti@2.6.1_postc_73b0d079a764cbb5c51135d704e2206c/node_modules:/proc/cygdrive/e/web/apk/RuoYi-Vue-Wechat/moltbot/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
